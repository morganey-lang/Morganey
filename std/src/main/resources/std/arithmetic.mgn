def succ := \n.f.x. f (n f x)

/* Note that in the Church encoding,
 * pred 0 = 0
 */
def pred := \n.f.x. n (\g.h. h (g f)) (\u. x) (\u. u)

def plus := \m.n.f.x. (m f) (n f x)

/* Note that in the Church encoding,
 * minus m n = if (m < n) 0
 */
def minus := \m.n. n pred m

def mult := \m.n.f. m (n f)

