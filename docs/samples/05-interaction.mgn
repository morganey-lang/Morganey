load std.prelude

stringAsNumber := \xs. (Y (\rec.xs. (isnil xs) 0 (plus (minus (head xs) '0') (mult (rec (tail xs)) 10)))) (reverse xs)

isnumber := \x. and (le '0' x) (le x '9')

isspace := eq ' '

// TODO: fix hanging of the interactive sample
main := plus (stringAsNumber (takeWhile isnumber testInput)) (stringAsNumber (dropWhile isspace (dropWhile isnumber testInput)))
